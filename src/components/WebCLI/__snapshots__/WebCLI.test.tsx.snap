exports[`WebCLI > renders correctly 1`] = `
<div
  class="webcli is-dark"
  data-testid="WebCLI"
>
  <div
    class="webcli__output"
    style="height: 0px;"
  >
    <div
      aria-hidden="true"
      class="webcli__output-dragarea"
    >
      <div
        class="webcli__output-handle"
      />
    </div>
    <pre
      class="webcli__output-content"
      data-testid="output-content"
      style="height: 0px;"
    >
      <code
        data-testid="output-help"
      >
        Welcome to the Juju Web CLI - see the
         
        <a
          href="https://documentation.ubuntu.com/juju/latest/reference/juju-web-cli/"
          rel="noreferrer"
          target="_blank"
        >
          full documentation here
        </a>
        .
      </code>
    </pre>
  </div>
  <div
    class="webcli__input"
  >
    <div
      class="webcli__input-prompt"
    >
      $ juju
    </div>
    <form>
      <input
        aria-label="enter command"
        autocapitalize="none"
        autocomplete="off"
        autocorrect="off"
        class="webcli__input-input"
        name="command"
        placeholder="enter command"
        spellcheck="false"
        type="text"
      />
    </form>
    <div
      class="webcli__input-help"
    >
      <i
        aria-label="Display help"
        class="p-icon--help is-light"
        role="button"
        tabindex="0"
      />
    </div>
  </div>
</div>
`;
